{
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "mapRoot": "/",
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "outDir": "../dist/",
    "rootDir": ".",
    "sourceMap": true,
    "target": "es5",
    "inlineSources": true
  },
  "filesGlob": [
    "./**/*.ts"
  ],
  "files": [
    "./app/app.component.ts",
    "./app/authentication.service.ts",
    "./app/environment.ts",
    "./app/fse-table/fse-cell/fse-cell.component.spec.ts",
    "./app/fse-table/fse-cell/fse-cell.component.ts",
    "./app/fse-table/fse-cell/index.ts",
    "./app/fse-table/fse-table-content.ts",
    "./app/fse-table/fse-table.component.spec.ts",
    "./app/fse-table/fse-table.component.ts",
    "./app/fse-table/index.ts",
    "./app/fse-table/shared/column.ts",
    "./app/fse-table/shared/keymap.ts",
    "./app/index.ts",
    "./app/shared/index.ts",
    "./app/student.service.ts",
    "./app/student.ts",
    "./app/with-crid-request-options.ts",
    "./main.ts",
    "./system-config.ts",
    "./typings.d.ts"
  ],
  "atom": {
    "rewriteTsconfig": true
  }
}
